{
  "name": "groot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon index.js",
    "serve": "PORT=8081 vue-cli-service serve",
    "dev-start": "concurrently \"npm run start\" \"npm run serve\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:node": "jest --config app/jest.config.js app --no-cache --detectOpenHandles --json --outputFile=./coverage/coverage.json",
    "test:vue": "jest --updateSnapshot --projects src --no-cache --detectOpenHandles --json --outputFile=./coverage/coverage.json",
    "test": "npm run test:node && npm run test:vue"
  },
  "dependencies": {
    "@gofynd/fdk-client-javascript": "^1.4.7",
