    "express": "^4.19.2",
    "fdk-extension-javascript": "git+https://github.com/gofynd/fdk-extension-javascript.git#v0.7.6",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/generator": "7.16.0",
