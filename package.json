    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.3.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-polyfill": "6.26.0",
    "concurrently": "^8.2.2",
